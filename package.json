{
  "name": "vite-plugin-index-gen",
  "version": "0.1.1",
  "description": "A Vite plugin to generate index.ts files",
  "author": "ze.dang",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "index-gen": "dist/cjs/cli.cjs"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "vitest",
    "build": "tsup"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20.12.13",
    "memfs": "^4.9.2",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}
